{
    "rules": {
        "new-parens": true,
        "no-arg": true,
        "no-bitwise": true,
        "no-conditional-assignment": true,
        "no-consecutive-blank-lines": false,
        "no-console": false,
        "semicolon": false,
        "indent": [
            true,
            "spaces",
            2
        ],
        "no-async-without-await": false
    }
}